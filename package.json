{
  "name": "@scope/package-name",
  "version": "2.0.2",
  "description": "Parse in-string variables",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "commit": "npx git-cz",
    "build": "yarn run clean && tsc -p ./",
    "clean": "rimraf dist",
    "test": "jest --config jestconfig.json",
    "lint": "eslint --ext .js,.ts --ignore-path .gitignore src/",
    "lint:fix": "yarn run lint --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "prettiest": "yarn run prettier:fix && yarn run lint:fix",
    "preversion": "yarn run build && yarn test",
    "prepublishOnly": "yarn run build && yarn test",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scope/package-name.git"
  },
  "author": "TMUniversal <me@tmuniversal.eu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scope/package-name/issues"
  },
  "homepage": "https://github.com/scope/package-name#readme",
  "devDependencies": {
    "@tmuniversal/ts": "^1.1.0",
    "@tmware/eslint-config": "^1.0.0",
    "@tmware/prettier-config": "^1.0.1",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "cz-conventional-changelog": "^3.2.1",
    "eslint": "^7.10.0",
    "eslint-plugin-jest": "^24.0.2",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "rimraf": "^3.0.2",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run prettiest && yarn run lint",
      "pre-push": "yarn run test"
    }
  },
  "keywords": [],
  "prettier": "@tmware/prettier-config"
}
