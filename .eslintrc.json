{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": ["@tmware/eslint-config-typescript"],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 12,
    "tsconfigRootDir": ".",
    "project": "./tsconfig.json"
  },
  "rules": {
    "no-console": 1,
    "no-undef": 1
  }
}
